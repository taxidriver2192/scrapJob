{
  "compilerOptions": {
    "target": "ES2023",
    "module": "none",
    "lib": ["DOM", "DOM.Iterable", "ES2023"],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "declaration": false,
    "outDir": "internal/scraper/scripts/dist",
    "types": []
  },
  "include": [
    "internal/scraper/scripts/src/*.ts"
  ],
  "exclude": [
    "internal/scraper/scripts/dist/**/*",
    "node_modules",
    "**/*.js"
  ]
}
